<mapper namespace="com.bbs.mapper.InReplayMapper">
        <select id="findAllInReplayByNoteIdAndReplayId" parameterType="int" resultType="InReplay">
                select * from inReplays where noteId=#{0} and replayId=#{1}
        </select>
</mapper>